.mapColumn
    width            : 100%
    background-color : #dddddd
    border           : 1px solid #c9c9c9
    font-family      : Arial
    font-size        : 12px
    padding          : 6px
    display          : table-row

    label
        display:    table-cell
        width:      230px
        color:      #000000
        padding:    6px
        border-top: 1px solid #d2d2d2

    label.assigned
        color: #909090

    .data
        display:    table-cell
        color:      #600080
        padding:    6px
        border-top: 1px solid #d2d2d2

    .data.assigned
        color: #909090

    .mapBox
        display:     table-cell
        width:       200px
        height:      100%
        border-left: 1px solid #d2d2d2
        padding:     6px
        border-top:  1px solid #d2d2d2
        color:       red
        cursor:     pointer

    .mapBoxPlus
        background-color: #F5F5F5
        display:    table-cell
        width:      30px
        padding:    6px
        text-align: center
        cursor:     pointer

    .mapBox.assigned
        color: #909090

#knownColumns

    position: fixed
    right:    10px
    top:      20px
    width:    230px
    height:   90%
    overflow: auto

    .knownTitle

        background-color: #060606
        padding-top:    8px
        padding-bottom: 8px
        padding-left:   8px
        padding-right:  8px
        text-align:     center
        color:          #E4B023

    .knownItem

        background-color: #1E1E1E
        color:            #B69A69
        padding-top:      4px
        padding-bottom:   4px
        padding-left:     8px
        padding-right:    8px
        border-top:       1px solid #404040
        cursor:           pointer

    .assigned

        color: #808080

    .selected

        background-color: #154415
        color:            #F9F9F9



