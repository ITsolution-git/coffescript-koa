.mapColumn
    width            : 830px
    background-color : #dddddd
    border           : 1px solid #c9c9c9
    font-family      : Arial
    font-size        : 12px
    padding          : 6px
    display          : table-row


    label
        display: table-cell
        width: 230px
        color: #000000

        padding: 6px
        border-top       : 1px solid #bbbbbb

    .data
        display: table-cell
        width: 380px
        color: #600080
        padding: 6px
        border-top       : 1px solid #bbbbbb

    .mapBox
        display     : table-cell
        width       : 200px
        height      : 100%
        border-left : 1px solid #d2d2d2
        padding: 6px
        color       : red


