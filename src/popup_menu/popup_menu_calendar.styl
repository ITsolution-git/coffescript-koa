.PopupMenuCal
	position:       absolute
	top:            0px
	left:           0px
	width:          300px
	z-index:        5000
	vertical-align: baseline
	margin:         0
	padding:        0
	border:         0
	cursor:         arrow
	box-sizing:     content-box
	background-color: rgba(30, 30, 30, 1.0)

	-webkit-touch-callout: none
	-moz-touch-callout:    none
	-ms-touch-callout:     none
	-o-touch-callout:      none
	touch-callout:         none

	webkit-tap-highlight-color: rgb(0,0,0)
	tap-highlight-color:        rgb(0,0,0)

	-webkit-text-size-adjust: none
	-moz-text-size-adjust:    none
	-o-text-size-adjust:      none
	text-size-adjust:         none

	-webkit-user-select: none
	-moz-user-select:    none
	-o-user-select:      none
	user-select:         none

	float:           none
	list-style:      disc outside none
	text-decoration: none
	font-size:       100%
	outline:         none

	box-shadow: 6px 6px 18px 4px rgba(0,0,0,0.75);
	border: 1px solid rgba(40,40,40,0.75)

.PopupCalendar

	width:            350px
	height: 		  340px

	opacity:          100
	position:         relative
	padding:          0
	margin:           0
	height:           auto
	display:          block
	background-color: rgba(30, 30, 30, 1.0)
	font-weight: 	  600
	font-size:		  16px
	border:			  0px
	text-align:		  left
	text-indent:	  0
	color:			  #eeeeee

	th
		width: 50px
		border: 1px solid #bbbbbb
		background-color: #dddddd
		text-align: center
		vertical-align: bottom
		color: #202020
		height: 24px
		font-size: 0.8vw
		padding-bottom: 2px
		cursor: no-drop

	td
		width: 50px
		height: 50px
		// border: 1px solid #bbbbbb
		background-color: #f2f2f2
		color: #202020
		vertical-align: middle
		text-align: center
		cursor: pointer

	td.diffMonth

		color: #aaaaaa

	td.today

		color: rgba(232, 100, 50, 1.0)
		font-weight: 700

	td.selected

		background-color: rgba(243, 146, 62, 1.0)
		color: #202020
		cursor: no-drop
		border: 1px solid rgba(243, 146, 62, 1.0)

	td.sun
	td.sat
		background-color: #e5e5e5
		color: #404040

	td.diffMonth.sun
	td.diffMonth.sat
		color: #aaaaaa

	td:hover
		color:      rgba(255,255,255,1)
		background-color: #404040

	td.message
		height: 24px
		color: #808080
		font-weight: 300
		border-top: 1px solid #bbbbbb
		font-size: 0.8vw
